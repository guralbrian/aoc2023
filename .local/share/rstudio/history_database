1701866248223:#function first splits  seed range into multiple parts such that every part is inside one interval
1701866248237:#afterwards, we add the offset to map it
1701866248253:map_seeds <- function(s_int, m) {
1701866248265:res <- m[s_int[1] <= m[,2] & m[,1] <= s_int[2], , drop = FALSE]
1701866248274:res[cbind(c(1, nrow(res)), 1:2)] <- s_int
1701866248284:res[,1:2] + res[,3]
1701866248293:}
1701866249225:find_location <- function(sr) { #find closest location given a matrix with seed ranges
1701866249239:for (m in alm) sr <- do.call(rbind, apply(sr, 1, map_seeds, m = m, simplify = F))
1701866249251:return(min(sr))
1701866249262:}
